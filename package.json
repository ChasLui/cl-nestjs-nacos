{
  "name": "cl-nestjs-nacos",
  "version": "0.0.0",
  "description": "NestJS 集成 Nacos 服务注册和配置中心",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaslui/nestjs-nacos.git"
  },
  "homepage": "https://github.com/chaslui/nestjs-nacos",
  "license": "MIT",
  "scripts": {
    "dev": "vite --config ./playground/vite.config.ts",
    "generate": "vite --config ./playground/vite.config.ts build",
    "build": "tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "coverage": "vitest run --coverage",
    "prepare": "husky install",
    "release": "pnpm test && npx standard-version && git push --follow-tags && npm publish --access public"
  },
  "lint-staged": {
    "*.ts": "eslint --fix",
    "*": "prettier -w -u"
  },
  "keywords": [
    "nestjs",
    "nacos",
    "service-discovery",
    "configuration-management",
    "microservices",
    "typescript",
    "service-registry",
    "config-center",
    "distributed-systems",
    "alibaba-nacos"
  ],
  "author": "ChasLui <chaslui@outlook.com> (https://twitter.com/chaslui1)",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@babel/types": "^7.28.2",
    "@eslint/js": "^9.34.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@types/debug": "^4.1.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.3.0",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "sinon": "^21.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.9.2",
    "unbuild": "^3.6.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "bugs": {
    "url": "https://github.com/chaslui/nestjs-nacos/issues"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "cheerio": "^1.1.2",
    "debug": "^4.4.1",
    "nacos": "^2.6.0",
    "os": "^0.1.2",
    "properties-reader": "^2.3.0",
    "xml2js": "^0.6.2",
    "js-yaml": "^4.1.0",
    "json5": "^2.2.3",
    "jsonc-parser": "^3.3.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "reflect-metadata": "^0.1.13"
  }
}
